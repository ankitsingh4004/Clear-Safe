<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.workorder.app.workorderapplication.activity.MaterialUsedList">
    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:theme="@style/AppTheme.AppBarOverlay"
        android:id="@+id/appBarLayout2">
        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay">
        </android.support.v7.widget.Toolbar>
    </android.support.design.widget.AppBarLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rl0"
        android:layout_below="@+id/appBarLayout2"
        android:layout_alignParentStart="true">
        <android.support.v7.widget.SearchView
            android:id="@+id/search_materialUsed"
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="11dp"
            android:background="@drawable/list_table"
            app:defaultQueryHint="Search"
            app:searchIcon="@drawable/ic_search_black_24dp"
            android:layout_margin="5dp"
            android:elevation="10dp"
            style="@style/SearchView"
            app:queryHint="Search">
        </android.support.v7.widget.SearchView>
        <ImageButton
            android:id="@+id/create_material"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="14dp"
            android:background="#ffffff"
            android:src="@drawable/create_allocation" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/rl0"
        android:layout_marginTop="5dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        >
        <android.support.v7.widget.RecyclerView
            android:id="@+id/recycler_materialUsed"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            >


        </android.support.v7.widget.RecyclerView>
    </RelativeLayout>
</RelativeLayout>
