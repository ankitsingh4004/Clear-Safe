<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

    <android.support.v7.widget.CardView
        android:id="@+id/card_work"
        android:layout_width="match_parent"
        android:layout_height="226dp"
        android:layout_margin="7dp"
        android:foreground="?android:attr/selectableItemBackground"
        app:cardCornerRadius="5dp"
        app:cardElevation="5dp"
        app:cardMaxElevation="10dp">

        <RelativeLayout
            android:id="@+id/rl1"
            android:layout_width="match_parent"
            android:layout_height="225dp"
            android:background="@drawable/card_border"
            android:paddingBottom="15dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="5dp">

            <LinearLayout
                android:id="@+id/lL1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="25dp"
                    android:fontFamily="serif"
                    android:text="Asset"
                    android:textColor="@color/colorPrimaryDark" />

                <TextView
                    android:id="@+id/asset_name"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:fontFamily="serif"
                    android:text="Water Pump"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lL2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="25dp"
                    android:fontFamily="serif"
                    android:text="Contractor"
                    android:textColor="@color/colorPrimaryDark" />

                <TextView
                    android:id="@+id/assigned_name"
                    android:layout_width="100dp"
                    android:layout_height="37dp"
                    android:fontFamily="serif"
                    android:text="Rahul Kumar"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lL3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="25dp"
                    android:fontFamily="serif"
                    android:text="Client"
                    android:textColor="@color/colorPrimaryDark" />

                <TextView
                    android:id="@+id/client_name"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:fontFamily="serif"
                    android:text="Ahuja Pvt Ltd."
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lL4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/lL3"
                android:layout_alignTop="@+id/lL6"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="25dp"
                    android:fontFamily="serif"
                    android:text="Date Raised"
                    android:textColor="@color/colorPrimaryDark" />

                <TextView
                    android:id="@+id/date_rasised"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:fontFamily="serif"
                    android:text="2017-11-27"
                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lL5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignStart="@+id/lL1"
                android:layout_marginBottom="0dp"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="25dp"
                    android:fontFamily="serif"
                    android:text="Status"
                    android:textColor="@color/colorPrimaryDark" />

                <TextView
                    android:id="@+id/status"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:fontFamily="serif"
                    android:text="0"
                    android:textColor="@color/black" />
            </LinearLayout>



            <LinearLayout
                android:id="@id/lL8"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rl"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="11dp"
                android:layout_marginRight="10dp">
                <ImageButton
                    android:id="@+id/materialused"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#fff"
                    android:src="@drawable/ic_assignment_turned_in_black_24dp"
                    android:visibility="invisible" />
                <ImageButton
                    android:id="@+id/addWorker"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#fff"
                    android:layout_marginLeft="10dp"
                    android:src="@drawable/workerlist"
                    android:visibility="invisible" />

                <ImageButton
                    android:id="@+id/updateworkOrder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/addWorker"
                    android:layout_marginLeft="10dp"
                    android:background="#fff"
                    android:src="@drawable/edit" />

                <ImageButton
                    android:id="@+id/deleteWorkOrder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/updateworkOrder"
                    android:layout_marginLeft="10dp"
                    android:background="#fff"
                    android:src="@drawable/delete" />
                <ImageButton
                    android:id="@+id/histroyWorkOrder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/deleteWorkOrder"
                    android:layout_marginLeft="10dp"
                    android:background="#fff"
                    android:src="@drawable/ic_history_black_24dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/lL5"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:orientation="horizontal">


            </LinearLayout>

            <LinearLayout
                android:id="@+id/rl9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="40dp"
                android:gravity="center"
                android:orientation="horizontal">

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lL6"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/lL7"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="25dp"
                    android:fontFamily="serif"
                    android:text="WO ID"
                    android:textColor="@color/colorPrimaryDark" />

                <TextView
                    android:id="@+id/workOrderId"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:fontFamily="serif"
                    android:text="017"

                    android:textColor="@color/black" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lL7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignStart="@+id/lL1"
                android:layout_marginTop="73dp"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:layout_width="100dp"
                    android:layout_height="25dp"
                    android:fontFamily="serif"
                    android:text="WO NO."
                    android:textColor="@color/colorPrimaryDark" />

                <TextView
                    android:id="@+id/work_order_no"
                    android:layout_width="100dp"
                    android:layout_height="35dp"
                    android:fontFamily="serif"
                    android:text="011"
                    android:textColor="@color/black" />
            </LinearLayout>
        </RelativeLayout>

    </android.support.v7.widget.CardView>
</LinearLayout>