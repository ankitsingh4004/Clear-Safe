<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    tools:context="com.workorder.app.workorderapplication.activity.Asset">    <android.support.design.widget.AppBarLayout        android:id="@+id/appBarLayout5"        android:layout_width="match_parent"        android:layout_height="55dp"        android:background="@color/colorPrimary"        android:theme="@style/AppTheme.AppBarOverlay">        <android.support.v7.widget.Toolbar            android:id="@+id/toolbar"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="?attr/colorPrimary"            app:popupTheme="@style/AppTheme.PopupOverlay"></android.support.v7.widget.Toolbar>    </android.support.design.widget.AppBarLayout>    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="57dp">        <FrameLayout            android:layout_width="match_parent"            android:layout_height="match_parent">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:orientation="vertical">                <RelativeLayout                    android:id="@+id/RLclien"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp">                    <TextView                        android:id="@+id/tvclient"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:fontFamily="serif"                        android:text="Client"                        android:textColor="@color/colorPrimaryDark"                        android:textSize="18sp" />                    <Spinner                        android:id="@+id/spinner_client"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:layout_centerInParent="true"                        android:layout_toRightOf="@+id/tvclient"                        android:backgroundTint="@color/colorPrimaryDark"                        android:spinnerMode="dropdown"></Spinner>                </RelativeLayout>                <android.support.design.widget.TextInputLayout                    android:id="@+id/TILClientNo"                    android:layout_width="170dp"                    android:layout_height="wrap_content"                    android:layout_below="@+id/RLclien"                    android:layout_marginTop="5dp">                    <EditText                        android:id="@+id/clientNo"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:fontFamily="serif"                        android:hint="Client No."                        android:textSize="18sp" />                </android.support.design.widget.TextInputLayout><!--                <android.support.design.widget.TextInputLayout                    android:id="@+id/TILAssetId"                    android:layout_width="170dp"                    android:layout_height="wrap_content"                    android:layout_below="@+id/RLclien"                    android:layout_alignParentRight="true"                    android:layout_marginTop="5dp">                    <EditText                        android:id="@+id/asset_id"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:fontFamily="serif"                        android:hint="Asset Id"                        android:inputType="number"                        android:textSize="18sp" />                </android.support.design.widget.TextInputLayout>-->                <android.support.design.widget.TextInputLayout                    android:id="@+id/TILAssetName"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/RLclien"                    android:layout_alignParentRight="true"                    android:layout_marginTop="5dp">                    <EditText                        android:id="@+id/asset_name"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:fontFamily="serif"                        android:hint="Asset Name"                        android:textSize="18sp" />                </android.support.design.widget.TextInputLayout>                <RelativeLayout                    android:id="@+id/RLAssetType"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/TILAssetName">                    <TextView                        android:id="@+id/tvcassetType"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:fontFamily="serif"                        android:text="Asset Type"                        android:textColor="@color/colorPrimaryDark"                        android:textSize="18sp" />                    <Spinner                        android:id="@+id/spinner_asset_type"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:layout_centerInParent="true"                        android:layout_toRightOf="@+id/tvcassetType"                        android:backgroundTint="@color/colorPrimaryDark"                        android:spinnerMode="dropdown"></Spinner>                </RelativeLayout>                <RelativeLayout                    android:id="@+id/RLAssetStatus"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/RLAssetType"                    android:layout_marginTop="5dp">                    <TextView                        android:id="@+id/tvcassetStatus"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:fontFamily="serif"                        android:text="Asset Status"                        android:textColor="@color/colorPrimaryDark"                        android:textSize="18sp" />                    <Spinner                        android:id="@+id/spinner_asset_status"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:layout_centerInParent="true"                        android:layout_toRightOf="@+id/tvcassetStatus"                        android:backgroundTint="@color/colorPrimaryDark"                        android:spinnerMode="dropdown"></Spinner>                </RelativeLayout>                <RelativeLayout                    android:id="@+id/RLPriority"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/RLAssetStatus"                    android:layout_marginTop="10dp">                    <TextView                        android:id="@+id/tvPriority"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:fontFamily="serif"                        android:text="Priority"                        android:textColor="@color/colorPrimaryDark"                        android:textSize="18sp" />                    <Spinner                        android:id="@+id/spinner_priority"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:layout_centerInParent="true"                        android:layout_toRightOf="@+id/tvPriority"                        android:backgroundTint="@color/colorPrimaryDark"                        android:spinnerMode="dropdown"></Spinner>                </RelativeLayout>                <RelativeLayout                    android:id="@+id/RLWarningLevel"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/RLPriority"                    android:layout_marginTop="10dp">                    <TextView                        android:id="@+id/tvWarning"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:fontFamily="serif"                        android:text="Warning Level"                        android:textColor="@color/colorPrimaryDark"                        android:textSize="18sp" />                    <Spinner                        android:id="@+id/spinner_warning_level"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:layout_centerInParent="true"                        android:layout_toRightOf="@+id/tvWarning"                        android:backgroundTint="@color/colorPrimaryDark"                        android:spinnerMode="dropdown"></Spinner>                </RelativeLayout>                <RelativeLayout                    android:id="@+id/RlProcessType"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/RLWarningLevel"                    android:layout_marginTop="10dp">                    <TextView                        android:id="@+id/tvProcesstype"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="5dp"                        android:fontFamily="serif"                        android:text="Process Type"                        android:textColor="@color/colorPrimaryDark"                        android:textSize="18sp" />                    <Spinner                        android:id="@+id/spinner_process_type"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:layout_centerInParent="true"                        android:layout_toRightOf="@+id/tvProcesstype"                        android:backgroundTint="@color/colorPrimaryDark"                        android:spinnerMode="dropdown"></Spinner>                </RelativeLayout>                <LinearLayout                    android:layout_below="@+id/RlProcessType"                    android:layout_marginTop="10dp"                    android:layout_width="match_parent"                    android:layout_height="match_parent">                    <android.support.design.widget.TextInputLayout                        android:id="@+id/TILContractNo"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_below="@+id/TILdescription"                        android:layout_weight="1">                        <EditText                            android:id="@+id/contract_number"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:fontFamily="serif"                            android:hint="Contract Number"                            android:textSize="18sp" />                    </android.support.design.widget.TextInputLayout>                    <android.support.design.widget.TextInputLayout                        android:id="@+id/TILloadDate"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_below="@+id/TILdescription"                        android:layout_alignParentRight="true"                        android:layout_weight="1">                        <EditText                            android:id="@+id/load_date"                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:drawableLeft="@drawable/date"                            android:focusable="false"                            android:fontFamily="serif"                            android:hint="Load Date"                            android:inputType="date"                            android:maxLength="10"                            android:textSize="18sp" />                    </android.support.design.widget.TextInputLayout>                </LinearLayout>                <android.support.design.widget.TextInputLayout                    android:id="@+id/TILdescription"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_below="@+id/RlProcessType"                    android:layout_alignParentRight="true"                    android:layout_marginTop="5dp">                    <EditText                        android:id="@+id/txt_description"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:fontFamily="serif"                        android:hint="Description"                        android:textSize="18sp" />                </android.support.design.widget.TextInputLayout>            </LinearLayout>        </FrameLayout>    </ScrollView>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="40dp"        android:layout_gravity="bottom">        <Button            android:id="@+id/btn_previous"            android:background="@color/colorPrimary"            android:layout_weight="1"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:elevation="20dp"            android:fontFamily="serif"            android:paddingLeft="20dp"            android:paddingRight="20dp"            android:text="Back"            android:textAllCaps="true"            android:textColor="#fff"            android:textSize="16sp" />        <View            android:layout_width="1dp"            android:layout_height="match_parent"/>        <Button            android:id="@+id/btn_next"            android:layout_weight="1"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:background="@color/colorPrimary"            android:elevation="20dp"            android:fontFamily="serif"            android:paddingLeft="20dp"            android:paddingRight="20dp"            android:text="Next"            android:textAllCaps="false"            android:textColor="#fff"            android:textSize="16sp" />    </LinearLayout></FrameLayout>